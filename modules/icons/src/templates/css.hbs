@font-face {
  font-family: "{{fontName}}";
  src: {{{src}}};
}

{{baseSelector}} {
  line-height: 1;
}

{{#each codepoints}}
.{{../classPrefix}}{{@key}}:before {
  content: "\\{{this}}";
  font-family: {{../fontName}} !important;
  font-style: normal;
  font-weight: normal !important;
  vertical-align: top;
}
{{/each}}